{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
  },
  "overrides": [{
    "files": ["./*.js", "./src/_data/**/*.js"],
    "rules": {
      "import/no-extraneous-dependencies": ["error", {
        "devDependencies": true
      }]
    }
  }]
}
